{
    "name": "@gjsify/stream",
    "version": "0.0.1",
    "description": "Node.js stream module for Gjs",
    "type": "module",
    "main": "lib/cjs/index.js",
    "module": "lib/esm/index.js",
    "types": "lib/types/index.d.js",
    "exports": {
        ".": {
            "import": "./lib/esm/index.js",
            "default": "./lib/esm/index.js",
            "require": "./lib/cjs/index.js"
        },
        "./consumers": {
            "import": "./lib/esm/consumers/index.js",
            "default": "./lib/esm/consumers/index.js",
            "require": "./lib/cjs/consumers/index.js"
        },
        "./promises": {
            "import": "./lib/esm/promises/index.js",
            "default": "./lib/esm/promises/index.js",
            "require": "./lib/cjs/promises/index.js"
        },
        "./web": {
            "import": "./lib/esm/web/index.js",
            "default": "./lib/esm/web/index.js",
            "require": "./lib/cjs/web/index.js"
        }
    },
    "*": {
        "": [
            "lib/types/index.d.js"
        ],
        "consumers": [
            "lib/types/consumers/index.d.js"
        ],
        "promises": [
            "lib/types/promises/index.d.js"
        ],
        "web": [
            "lib/types/web/index.d.js"
        ]
    },
    "scripts": {
        "clear": "rm -rf lib tsconfig.tsbuildinfo",
        "print:name": "echo '@gjsify/stream'",
        "build": "yarn print:name && yarn build:gjsify && yarn build:types",
        "build:gjsify": "gjsify build --library 'src/**/*.{ts,js}' --exclude 'src/**/*.spec.{mts,ts}' 'src/test.{mts,ts}'",
        "build:types": "tsc --project tsconfig.types.json || exit 0",
        "build:test": "yarn build:test:gjs && yarn build:test:node",
        "build:test:gjs": "gjsify build src/test.mts --app gjs --outfile test.gjs.mjs",
        "build:test:node": "gjsify build src/test.mts --app node --outfile test.node.mjs",
        "test": "yarn print:name && yarn build:gjsify && yarn build:test && yarn test:node && yarn test:gjs",
        "test:gjs": "gjs -m test.gjs.mjs",
        "test:node": "node test.node.mjs"
    },
    "keywords": [
        "gjs",
        "node",
        "stream"
    ],
    "devDependencies": {
        "@gjsify/cli": "workspace:^",
        "@gjsify/unit": "workspace:^",
        "@types/node": "^18.11.18",
        "typescript": "^5.0.0-dev.20230202"
    },
    "dependencies": {
        "@gjsify/deno_std": "workspace:^"
    }
}
