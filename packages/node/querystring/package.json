{
    "name": "@gjsify/querystring",
    "version": "0.0.1",
    "description": "Node querystring module for gjsify",
    "main": "lib/index.cjs",
    "module": "lib/index.mjs",
    "type": "module",
    "scripts": {
        "build": "node --experimental-import-meta-resolve esbuild.mjs",
        "test": "yarn run build && yarn run test:node && yarn run test:gjs",
        "test:gjs": "gjs -m test.gjs.js",
        "test:node": "node test.node.js"
    },
    "keywords": [
        "gjs",
        "node",
        "fs"
    ],
    "devDependencies": {
        "@gjsify/esbuild-plugin-gjsify": "workspace:^",
        "@gjsify/unit": "workspace:^",
        "@types/inherits": "^0.0.30",
        "@types/node": "^18.7.14"
    },
    "dependencies": {
        "is-arguments": "^1.1.1",
        "is-generator-function": "^1.0.10",
        "is-typed-array": "^1.1.9",
        "util": "^0.12.4",
        "which-typed-array": "^1.1.8"
    }
}
