{
    "name": "@gjsify/soup-3.0",
    "version": "0.0.1",
    "description": "Extended Soup-3.0 module for Gjsify",
    "main": "lib/index.js",
    "module": "lib/index.js",
    "types": "lib/index.d.ts",
    "type": "module",
    "scripts": {
        "clear": "rm -rf lib tsconfig.tsbuildinfo ",
        "print:name": "echo '@gjsify/soup-3.0'",
        "build": "yarn print:name && yarn build:gjsify && yarn build:types",
        "build:gjsify": "gjsify build --library 'src/**/*.{ts,js}' --exclude 'src/**/*.spec.{mts,ts}' 'src/test.{mts,ts}'",
        "build:types": "tsc --emitDeclarationOnly || exit 0",
        "build:test": "gjsify build src/test.ts --app gjs --outfile test.gjs.js",
        "test": "yarn print:name && yarn build:gjsify && yarn build:test && yarn test:gjs",
        "test:gjs": "gjs -m test.gjs.js"
    },
    "keywords": [
        "gjs",
        "node",
        "console"
    ],
    "devDependencies": {
        "@gjsify/cli": "workspace:^",
        "@gjsify/unit": "workspace:^",
        "@types/node": "^18.16.16",
        "typescript": "^5.1.3"
    },
    "dependencies": {
        "@girs/gjs": "^3.0.3",
        "@girs/soup-3.0": "3.4.2-3.0.3"
    }
}
