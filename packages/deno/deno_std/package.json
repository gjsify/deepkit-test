{
    "name": "@gjsify/deno_std",
    "version": "0.0.1",
    "description": "Deno deno_std module for gjsify",
    "main": "lib/node/global.js",
    "module": "lib/node/global.mjs",
    "type": "module",
    "exports": {
        "./*": {
            "import": "./lib/*.js",
            "default": "./lib/*.js"
        }
    },
    "typesVersions": {
        "*": {
          "*": [
            "./lib/*.d.ts"
          ]
        }
      },
    "scripts": {
        "build": "node --experimental-import-meta-resolve esbuild.mjs",
        "test": "yarn run build && yarn run test:node && yarn run test:gjs",
        "test:gjs": "gjs -m test.gjs.js",
        "test:node": "node test.node.js"
    },
    "keywords": [
        "gjs",
        "node",
        "deno",
        "deno_std"
    ],
    "devDependencies": {
        "@gjsify/esbuild-plugin-gjsify": "workspace:^",
        "@gjsify/unit": "workspace:^",
        "@types/node": "^18.7.14"
    }
}
