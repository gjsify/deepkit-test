{
    "name": "@gjsify/esbuild-plugin-gjsify",
    "version": "0.0.1",
    "description": "Deepkit type compiler plugin for esbuild",
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "scripts": {
        "clear": "rm -rf dist",
        "print:name": "echo '@gjsify/esbuild-plugin-gjsify'",
        "build": "yarn run build:js && yarn run build:types",
        "build:js": "node esbuild.mjs",
        "build:types": "tsc --emitDeclarationOnly || exit 0"
    },
    "keywords": [
        "gjs",
        "node",
        "fs"
    ],
    "dependencies": {
        "@deepkit/core": "^1.0.1-alpha.75",
        "@deepkit/injector": "^1.0.1-alpha.75",
        "@deepkit/type": "^1.0.1-alpha.75",
        "@gjsify/assert": "workspace:^",
        "@gjsify/events": "workspace:^",
        "@gjsify/fs": "workspace:^",
        "@gjsify/globals": "workspace:^",
        "@gjsify/node-internal": "workspace:^",
        "@gjsify/os": "workspace:^",
        "@gjsify/process": "workspace:^",
        "@gjsify/require": "workspace:^",
        "@gjsify/resolve-npm": "workspace:^",
        "@gjsify/tty": "workspace:^",
        "@gjsify/zlib": "workspace:^",
        "assert": "^2.0.0",
        "constants-browserify": "^1.0.0",
        "core-js": "^3.25.2",
        "crypto-browserify": "^3.12.0",
        "esbuild-plugin-glob": "^2.1.0",
        "path-browserify": "^1.0.1",
        "punycode": "^2.1.1",
        "stream-browserify": "^3.0.0",
        "string_decoder": "^1.3.0",
        "web-streams-polyfill": "^3.2.1"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.185",
        "esbuild": "^0.15.8",
        "lodash": "^4.17.21",
        "typescript": "^4.8.3"
    }
}
