{
  "name": "@gjsify/require",
  "version": "0.0.1",
  "description": "require core module for gjs",
  "keywords": [
    "gjs",
    "node",
    "require"
  ],
  "author": "Pascal Garber",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "scripts": {
    "clear": "rm -rf lib",
    "build": "gjsify build --library src/**/*.{ts,js}",
    "build:test": "yarn run build:test:gjs && yarn run build:test:node",
    "build:test:gjs": "gjsify build src/test.ts --platform gjs --outfile test.gjs.mjs",
    "build:test:node": "gjsify build src/test.ts --platform node --outfile test.node.cjs",
    "test": "yarn run build && yarn run build:test && yarn run test:node && yarn run test:gjs",
    "test:gjs": "gjs -m test.gjs.mjs",
    "test:node": "node test.node.cjs"
  },
  "dependencies": {
    "@gjsify/process": "workspace:^",
    "@gjsify/resolve-npm": "workspace:^",
    "@gjsify/utils": "workspace:^"
  },
  "devDependencies": {
    "@gjsify/cli": "workspace:^",
    "@gjsify/unit": "workspace:^",
    "cowsay2": "^2.0.4"
  }
}
