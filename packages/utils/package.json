{
    "name": "@gjsify/utils",
    "version": "0.0.1",
    "description": "utils module for gjsify",
    "main": "lib/index.mjs",
    "module": "lib/index.mjs",
    "type": "module",
    "scripts": {
        "build": "yarn run build:js && yarn run build:types",
