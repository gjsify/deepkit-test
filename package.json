{
  "name": "gjsify",
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "yarn run build:esbuild; yarn run build:utils; yarn run build:unit; yarn run build:node",
    "build:esbuild": "yarn workspace @gjsify/esbuild-plugin-deepkit run build; yarn workspace @gjsify/esbuild-plugin-gjsify run build",
    "build:unit": "yarn workspace @gjsify/unit run build",
    "build:node": "yarn workspace @gjsify/process run build; yarn workspace @gjsify/tty run build; yarn workspace @gjsify/url run build; yarn workspace @gjsify/fs run build; yarn workspace @gjsify/os run build; yarn workspace @gjsify/globals run build",
    "build:types": "yarn workspace @gjsify/types run build",
    "build:utils": "yarn workspace @gjsify/utils run build",
    "build:all": "yarn workspaces foreach run build"
  },
  "author": "Pascal Garber <pascal@artandcode.studio>",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^4.8.2"
  },
  "packageManager": "yarn@3.2.1",
  "resolutions": {
    "@deepkit/core": "portal:../deepkit-framework/packages/core",
    "@deepkit/event": "portal:../deepkit-framework/packages/event",
    "@deepkit/stopwatch": "portal:../deepkit-framework/packages/stopwatch",
    "@deepkit/workflow": "portal:../deepkit-framework/packages/workflow",
    "@deepkit/app": "portal:../deepkit-framework/packages/app",
    "@deepkit/injector": "portal:../deepkit-framework/packages/injector",
    "@deepkit/type": "portal:../deepkit-framework/packages/type",
    "@deepkit/type-compiler": "portal:../deepkit-framework/packages/type-compiler",
    "@deepkit/type-spec": "portal:../deepkit-framework/packages/type-spec",
    "buffer": "^6.0.3"
  },
  "workspaces": [
    "packages/*",
    "packages/node/*",
    "packages/web/*",
    "packages/deno/*",
    "examples/*"
  ]
}
