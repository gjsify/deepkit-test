{
    "name": "@gjsify/esbuild-plugin-deepkit",
    "version": "0.0.1",
    "description": "Deepkit type compiler plugin for esbuild",
    "type": "commonjs",
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "yarn run build:js && yarn run build:types",
        "build:js": "node esbuild.js",
        "build:types": "tsc --emitDeclarationOnly"
    },
    "keywords": [
        "gjs",
        "node",
        "fs"
    ],
    "dependencies": {
        "@deepkit/type-compiler": "^1.0.1-alpha.74"
    },
    "devDependencies": {
        "esbuild": "^0.15.6",
        "typescript": "^4.8.2"
    }
}
