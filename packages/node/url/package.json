{
    "name": "@gjsify/url",
    "version": "0.0.1",
    "description": "url node module for gjsify",
    "main": "lib/index.mjs",
    "module": "lib/index.mjs",
    "type": "module",
    "scripts": {
        "build": "yarn run bundle:deno && yarn run patch:deno && yarn run build:esbuild",
        "build:esbuild": "node --experimental-import-meta-resolve esbuild.mjs",
        "bundle:deno": "rm -f src/url.deno.js && deno bundle https://deno.land/std/node/url.ts src/url.deno.js",
        "patch:deno": "node ./patch-deno.mjs",
        "test": "yarn run build && yarn run test:node && yarn run test:gjs",
        "test:gjs": "gjs -m test.gjs.js",
        "test:node": "node test.node.js"
    },
    "keywords": [
        "gjs",
        "node",
        "fs"
    ],
    "devDependencies": {
        "@gjsify/esbuild-plugin-gjsify": "workspace:^",
        "@gjsify/unit": "workspace:^"
    }
}
