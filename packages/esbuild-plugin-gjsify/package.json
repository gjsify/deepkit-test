{
    "name": "@gjsify/esbuild-plugin-gjsify",
    "version": "0.0.1",
    "description": "Deepkit type compiler plugin for esbuild",
    "type": "module",
    "main": "dist/index.mjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "yarn run build:js && yarn run build:types",
        "build:js": "node esbuild.cjs",
        "build:types": "tsc --emitDeclarationOnly"
    },
    "keywords": [
        "gjs",
        "node",
        "fs"
    ],
    "dependencies": {
        "@deepkit/app": "^1.0.1-alpha.74",
        "@deepkit/injector": "^1.0.1-alpha.74",
        "@deepkit/type": "^1.0.1-alpha.74",
        "@gjsify/events": "workspace:^",
        "@gjsify/fs": "workspace:^",
        "@gjsify/globals": "workspace:^",
        "@gjsify/os": "workspace:^",
        "@gjsify/process": "workspace:^",
        "@gjsify/require": "workspace:^",
        "@gjsify/tty": "workspace:^",
        "@gjsify/zlib": "workspace:^",
        "buffer": "^6.0.3",
        "core-js": "^3.25.0",
        "crypto-browserify": "^3.12.0",
        "deepmerge": "^4.2.2",
        "esbuild-plugin-alias": "^0.2.1",
        "path-browserify": "^1.0.1",
        "punycode": "^2.1.1",
        "querystring-es3": "^0.2.1",
        "stream-browserify": "^3.0.0",
        "string_decoder": "^1.3.0",
        "web-streams-polyfill": "^3.2.1"
    },
    "devDependencies": {
        "esbuild": "^0.15.6",
        "typescript": "^4.8.2"
    }
}
