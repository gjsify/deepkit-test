{
    "extends": "@tsconfig/deno/tsconfig.json",
    "compilerOptions": {
        "lib": ["DOM"],
        "module": "ESNext",
        "target": "ESNext",
        "moduleResolution": "Node",
        "outDir": "lib",
        "declarationDir": "lib",
        "rootDir": "src",
        "composite": true,
        "resolveJsonModule": false,
        "strict": false
    },
    "skipLibCheck": true,
    "allowJs": true,
    "checkJs": false,
    "reflection": false,
    "include": ["src/core/**/*.ts", "src/ext/**/*.ts", "src/ops/**/*.ts", "src/runtime/**/*.ts", "src/types/**/*.ts"],
    "exclude": []
  }