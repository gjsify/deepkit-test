{
  "name": "@gjsify/example-webgl-tutorial-06",
  "private": true,
  "version": "0.0.3",
  "description": "",
  "main": "dist/index.gjs.js",
  "type": "module",
  "scripts": {
    "clear": "rm -rf dist tsconfig.tsbuildinfo",
    "print:name": "echo '@gjsify/example-webgl-tutorial-06'",
    "start": "yarn start:gjs",
    "start:gjs": "yarn build:gjs && yarn run:gjs",
    "start:browser": "yarn build:browser && open-cli index.html",
    "run:gjs": "LD_LIBRARY_PATH=../../packages/web/webgl/build GI_TYPELIB_PATH=../../packages/web/webgl/build gjs -m dist/gjs.js",
    "build": "yarn print:name && yarn build:gjs && yarn build:browser",
    "build:gjs": "gjsify build src/gjs.ts --app gjs --outfile dist/gjs.js",
    "build:browser": "gjsify build src/browser.ts --app browser --outfile dist/browser.js"
  },
  "engines": {
    "node": "16",
    "gjs": "1.72.x"
  },
  "author": "Pascal Garber <pascal@artandcode.studio>",
  "license": "MIT",
  "devDependencies": {
    "@gjsify/cli": "workspace:^",
    "@types/gl-matrix": "^3.2.0",
    "@types/node": "^20.10.5",
    "open-cli": "^8.0.0"
  },
  "dependencies": {
    "@girs/gio-2.0": "2.78.0-3.2.6",
    "@girs/gjs": "^3.2.6",
    "@girs/gtk-4.0": "4.12.3-3.2.6",
    "@gjsify/webgl": "workspace:^",
    "gl-matrix": "^3.4.3"
  }
}
