{
    "name": "@girs/gwebgl-0.1",
    "version": "0.1.0-3.1.0",
    "description": "GJS TypeScript type definitions for Gwebgl-0.1, generated from library version 0.1.0",
    "type": "module",
    "module": "gwebgl-0.1.js",
    "main": "gwebgl-0.1.js",
    "typedoc": {
        "entryPoint": "./gwebgl-0.1.d.ts",
        "readmeFile": "./README.md",
        "displayName": "Gwebgl-0.1",
        "tsconfig": "./tsconfig.doc.json"
    },
    "exports": {
        "./ambient": "./gwebgl-0.1-ambient.d.ts",
        "./import": "./gwebgl-0.1-import.d.ts",
        ".": {
            "import": {
                "types": "./gwebgl-0.1.d.ts",
                "default": "./gwebgl-0.1.js"
            },
            "require": {
                "types": "./gwebgl-0.1.d.cts",
                "default": "./gwebgl-0.1.cjs"
            }
        }
    },
    "scripts": {
        "test": "yarn test:esm && yarn test:cjs",
        "test:esm": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit gwebgl-0.1.d.ts",
        "test:cjs": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit gwebgl-0.1.d.cts"
    },
    "dependencies": {
        "@girs/gjs": "3.2.6",
        "@girs/glib-2.0": "^2.78.0-3.2.6",
        "@girs/gobject-2.0": "^2.78.0-3.2.6"
    },
    "devDependencies": {
        "typescript": "*"
    },
    "keywords": [
        "Gir",
        "TypeScript",
        "types",
        "GObject-Introspection",
        "GJS",
        "Gwebgl-0.1"
    ],
    "author": "ts-for-gir",
