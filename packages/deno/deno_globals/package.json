{
    "name": "@gjsify/deno_globals",
    "version": "0.0.1",
    "description": "Deno globals module for gjsify",
    "main": "lib/index.mjs",
    "module": "lib/index.mjs",
    "type": "module",
    "scripts": {
        "build": "node --experimental-import-meta-resolve esbuild.mjs",
        "test": "yarn run build && yarn run test:deno && yarn run test:gjs",
        "test:gjs": "gjs -m test.gjs.js",
        "test:deno": "deno run test.gjs.js"
    },
    "keywords": [
        "gjs",
        "deno",
        "globals"
    ],
    "devDependencies": {
        "@gjsify/esbuild-plugin-gjsify": "workspace:^",
        "@gjsify/unit": "workspace:^",
        "@types/node": "^18.7.14"
    }
}
