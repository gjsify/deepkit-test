{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Deno",
    "compilerOptions": {
        "jsx": "react",
        "lib": [],
        "module": "ESNext",
        "target": "ESNext",
        "moduleResolution": "NodeNext",
        "outDir": "lib",
        "declarationDir": "lib",
        "rootDir": "src",
        "composite": true,
        "resolveJsonModule": false,
        "strict": false
    },
    "skipLibCheck": true,
    "allowJs": true,
    "checkJs": false,
    "reflection": false,
    "include": ["src/core/**/*.ts", "src/ext/**/*.ts", "src/ops/**/*.ts", "src/runtime/**/*.ts", "src/types/**/*.ts", "src/cli/js/*.ts", "src/cli/ops/*.ts", "src/*.ts", "src/cli/tsc/dts/lib.deno.*.d.ts"],
    "exclude": []
}