{
    "name": "@gjsify/fetch",
    "version": "0.0.1",
    "description": "web fetch module for gjsify",
    "main": "lib/index.mjs",
    "module": "lib/index.mjs",
    "type": "module",
    "scripts": {
        "build": "node --experimental-import-meta-resolve esbuild.mjs",
        "test": "yarn run build && yarn run test:node && yarn run test:gjs",
        "test:gjs": "gjs -m test.gjs.js",
        "test:node": "node test.node.js"
    },
    "keywords": [
        "gjs",
        "node",
        "fetch"
    ],
    "devDependencies": {
        "@gjsify/esbuild-plugin-gjsify": "workspace:^",
        "@gjsify/http": "workspace:^",
        "@gjsify/unit": "workspace:^",
        "@types/node": "^18.7.14"
    },
    "dependencies": {
        "data-uri-to-buffer": "^4.0.0",
        "fetch-blob": "^3.2.0",
        "formdata-polyfill": "^4.0.10"
    }
}
